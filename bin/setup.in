    "yes/")
        CC="ccache gcc"; export CC
        CXX="ccache g++"; export CXX
        ;;

    "/yes")
        CC="$ICECREAM_BINDIR/gcc"; export CC
        CXX="$ICECREAM_BINDIR/g++"; export CXX
        ;;

    "yes/yes")
        CC="ccache $ICECREAM_BINDIR/gcc"; export CC
        CXX="ccache $ICECREAM_BINDIR/g++"; export CXX
        ;;
esac

