+void ScXMLRubyContext::EndElement()
+{
+    if ( pTextContext )
+        pTextContext->AddRubyText( sBaseText.makeStringAndClear(), sRubyText.makeStringAndClear() );
+    else if ( pCellContext )
+        pCellContext->SetPhoneticText( sRubyText.makeStringAndClear() );
+}
