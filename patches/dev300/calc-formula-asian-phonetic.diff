+        xPortions.push_back ( PhoneticPortion( ncpa, ncpm, nccm ) );
+    }
+
+    maPhonetic = ScPhonetic ( nFontIdx, nAdditional, nRepeatedLength,
+                              maPhoneticString, xPortions );
