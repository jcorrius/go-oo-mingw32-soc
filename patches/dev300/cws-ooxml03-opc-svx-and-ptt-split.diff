+			SetPageNum( i, PPT_SLIDEPAGE );
+			SdPage* pPage = mpDoc->GetSdPage( i, PK_STANDARD );
+			AutoLayout eAutoLayout = AUTOLAYOUT_NONE;
+			const PptSlideLayoutAtom* pSlideLayout = GetSlideLayoutAtom();
