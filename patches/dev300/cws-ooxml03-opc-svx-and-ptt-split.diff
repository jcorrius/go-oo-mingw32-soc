+        DBG(printf("adj seq len: %d\n", int( aAdjustmentSeq.getLength() )));
+        if ( bPredefinedHandlesUsed )
+            EscherPropertyContainer::LookForPolarHandles( eShapeType, nAdjustmentsWhichNeedsToBeConverted );
