+sub command_value( $ )
+{
+    my ( $value ) = @_;
+
+    return "" if ( $value eq "" );
+    
+    return $value if ( $value =~ /^@/ );
+
+    my $command_val = $command_variables{$value};
+    if ( defined( $command_val ) ) {
+        return $command_val;
+    }
+    
+    return value( $value );
+}
