+	///////////////////////////////////////////////////////////////////
+	// Handzettel und Notiz-Seiten erzeugen 						 //
+	///////////////////////////////////////////////////////////////////
+	bOk = mpDoc->CreateMissingNotesAndHandoutPages();
+	if ( bOk )
+	{
+		for ( i = 0; i < mpDoc->GetSdPageCount( PK_STANDARD ); i++ )
+		{
+			////////////////////
