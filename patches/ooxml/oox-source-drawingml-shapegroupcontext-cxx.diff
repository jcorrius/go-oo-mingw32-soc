+
+Reference< XFastContextHandler > ShapeGroupContext::createFastChildContext( sal_Int32 aElementToken, const Reference< XFastAttributeList >& xAttribs ) throw (SAXException, RuntimeException)
+{
+	Reference< XFastContextHandler > xRet;
+
+	switch( aElementToken &(~NMSP_MASK) )
+	{
+	case XML_cNvPr:
+		mpGroupShapePtr->setId( xAttribs->getOptionalValue( XML_id ) );
+		mpGroupShapePtr->setName( xAttribs->getOptionalValue( XML_name ) );
+		break;
+	case XML_txBody:
+	{
+		ShapePtr pNewShape( new Shape( "com.sun.star.drawing.CustomShape" ) );
+		mpGroupShapePtr->addChild( pNewShape );
+		xRet.set( new TextBodyContext( getHandler(), pNewShape ) );
+		
+/*
+		Reference< XText > xText( mxShape, UNO_QUERY );
+		if( xText.is() )
+			xRet = new TextBodyContext( getHandler(), xText );
+*/
+		break;
+	}
+	case XML_ph:
+		mpGroupShapePtr->setSubType( xAttribs->getOptionalValueToken( XML_type, FastToken::DONTKNOW ) );
+		mpGroupShapePtr->setIndex( xAttribs->getOptionalValue( XML_idx ).toInt32() );
+		break;
+	// nvSpPr CT_ShapeNonVisual end
+
+	case XML_spPr:
+		xRet = new ShapePropertiesContext( this, mpGroupShapePtr );
+		break;
+/*
+	case XML_style:
+		xRet = new ShapeStyleContext( getParser() );
+		break;
+*/
+	case XML_grpSp:			// group shape
+		{
+			ShapePtr pNewGroupShape( new Shape( "com.sun.star.drawing.GroupShape" ) );
+			mpGroupShapePtr->addChild( pNewGroupShape );
+            xRet.set( new ShapeGroupContext( getHandler(), GetFilter(), aElementToken, pNewGroupShape ) );
+		}
+		break;
+	case XML_sp:			// shape
+	case XML_cxnSp:		// connector todo?
+		{
+			ShapePtr pNewShape( new Shape( "com.sun.star.drawing.CustomShape" ) );
+			mpGroupShapePtr->addChild( pNewShape );
+            xRet.set( new ShapeContext( getHandler(), pNewShape ) );
+		}
+		break;
+
+	case XML_pic:			// CT_Picture
+		{
+			ShapePtr pNewShape( new Shape( "com.sun.star.drawing.GraphicObjectShape" ) );
+			mpGroupShapePtr->addChild( pNewShape );
+            xRet.set( new GraphicShapeContext( getHandler(), pNewShape ) );
+		}
+		break;
+
+	case XML_graphicFrame:	// CT_GraphicalObjectFrame
+		{
+			ShapePtr pNewShape( new Shape( "com.sun.star.drawing.OLE2Shape" ) );
+			mpGroupShapePtr->addChild( pNewShape );
+            xRet.set( new GraphicalObjectFrameContext( getHandler(), pNewShape ) );
+		}
+		break;
+	}
+	if( !xRet.is() )
+		xRet.set( this );
+
+
+	return xRet;
+}
