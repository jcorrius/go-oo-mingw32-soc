# Localization patches to localize.sdf files: applied only when we have langpacks
Localize : SDFPatches
# Novell
NovellBase : LinuxCommon, NovellSplash, NovellOnly, NovellOnlyUnix, \
	     NotDebian, Lockdown, CalcSolver, AtkBridge
