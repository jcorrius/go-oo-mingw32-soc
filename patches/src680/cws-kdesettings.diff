+static const cppu::ImplementationEntry kImplementations_entries[] = 
+{
+    {
+        createKDEBackend,
+        KDEBackend::getBackendName,
+        KDEBackend::getBackendServiceNames,
+        cppu::createSingleComponentFactory,
+        NULL,
+        0
+    },
+	{ NULL }
+} ;
