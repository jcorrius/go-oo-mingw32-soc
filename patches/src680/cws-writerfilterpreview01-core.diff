+    std::auto_ptr < SwUnoInternalPaM > pEndPam( new SwUnoInternalPaM(*GetDoc()));
+	if(SwXTextRange::XTextRangeToSwPaM(aStartPam, xStart) && 
+        SwXTextRange::XTextRangeToSwPaM(*pEndPam, xEnd) )
+    {
+		m_pDoc->StartUndo( UNDO_START, NULL );
